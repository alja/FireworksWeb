<mvc:View
    controllerName="fw.controller.EventFilter"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns="sap.m"
    class="sapUiSizeCompact"
    >

   <Dialog class="sapUiSizeCompact" id="filterDialog" draggable="true" resizable="true" >

    <customHeader>
        <Bar>
            <contentLeft>
                <Button />
            </contentLeft>
            <contentMiddle>
                <Title text="Event Filter Dialog" />
            </contentMiddle>
        </Bar>
    </customHeader>
    <Panel id="plainPanel" headerText="Collection Filters">
    </Panel>
    <Panel id="hltPanel" headerText="HLT Filters">
    </Panel>
   </Dialog>
</mvc:View>
