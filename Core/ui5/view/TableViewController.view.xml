<mvc:View
    controllerName="fw.controller.TableViewController"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns="sap.m"
    class="sapUiSizeCompact"
    >

   <Dialog class="sapUiSizeCompact dialogStyle" id="viewController" draggable="true" resizable="true" > 

    <customHeader>
        <Bar>
            <contentMiddle>
                <Title text="View Controller" />
            </contentMiddle>
        </Bar>
    </customHeader>
        <Button id="info" text="Print Table to Log" press="print"/>
    <endButton>
        <Button tooltip="Close dialog or press Esc key" text="Close" press="onClose"/>
    </endButton>
    </Dialog>
</mvc:View>
